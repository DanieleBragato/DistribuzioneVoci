<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1">
         <children>
           <Label prefWidth="450.0" styleClass="label-header" text="Tabelle" textAlignment="CENTER" />
           <TableView fx:id="tabelledbTable" prefHeight="255.0" prefWidth="450.0">
              <columns>
                 <TableColumn fx:id="codiceTabColumn" prefWidth="130.0" text="Codice" />
                 <TableColumn fx:id="descrizioneTabColumn" prefWidth="310.0" text="Descrizione" />
              </columns>
           </TableView>
           <ButtonBar prefHeight="33.0" prefWidth="407.0">
              <buttons>
                 <Button mnemonicParsing="false" onAction="#handleNewTabDB" text="New.." />
                 <Button layoutX="368.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleEditTabDB" text="Edit.." />
                 <Button layoutX="348.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleDeleteTabDB" text="Delete" />
              </buttons>
              <VBox.margin>
                 <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
              </VBox.margin>
           </ButtonBar>
        </children>
</AnchorPane>

